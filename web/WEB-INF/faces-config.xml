<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.1"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">

    <application>
        <message-bundle>
            inne.messages
        </message-bundle>
    </application>    
    
    <navigation-rule>   
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>index.xhtml</to-view-id>        
             
        </navigation-case>   
    </navigation-rule> 
    
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>klienciTable</from-outcome>
            <to-view-id>/doradca/tableKlienci.xhtml</to-view-id>  
            <redirect />                  
        </navigation-case>      
    </navigation-rule>
    
    <navigation-rule>   
        <from-view-id>doradca/*</from-view-id>
        <navigation-case>
            <from-outcome>form2</from-outcome>
            <to-view-id>/doradca/formKredyty.xhtml</to-view-id>    
            <redirect />                
        </navigation-case>
    </navigation-rule>   

    <navigation-rule>   
        <from-view-id>doradca/*</from-view-id>
        <navigation-case>
            <from-outcome>xxx</from-outcome>
            <to-view-id>/doradca/tableKredyty.xhtml</to-view-id>       
            <redirect />   
        </navigation-case>
    </navigation-rule>  

    <navigation-rule>   
        <from-view-id>doradca/*</from-view-id>
        <navigation-case>
            <from-outcome>indexstart</from-outcome>
            <to-view-id>/doradca/formKlienci.xhtml</to-view-id>        
            <redirect />  
        </navigation-case>
        
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>index.xhtml</to-view-id>        
            <redirect />  
        </navigation-case>
        
    </navigation-rule> 
    
      
    
</faces-config>
